{
    "parserOptions": {
        "ecmaVersion": 11
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules":{
        "eqeqeq": ["error","always"],
        "no-unused-vars": "warn",
        "no-fallthrough": "off",
        "complexity": "off"
    },
    "globals": {
        "sabre": "readonly",
        "global": "readonly",
        "external": "readonly",
        "require": "readonly"
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "root": true
}
